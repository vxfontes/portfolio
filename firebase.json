{
  "hosting": {
    "site": "vxfontes",
    "public": "out",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "destination": "/projects/[id]/index.html",
        "regex": "^/projects/([^/]+?)(?:/)?$"
      }
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  }
}